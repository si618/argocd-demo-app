{
  "profiles": {
    "TodoApi": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:443;http://localhost:80"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
